{
  "manifest_version": 3,
  "name": "Speed Dial - Open Source",
  "version": "1.0.1",
  "description": "A new tab page with bookmarks from a selected folder.",
  "permissions": [
    "bookmarks",
    "storage"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "theme-loader.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ],
  "chrome_url_overrides": {
    "newtab": "src/newtab.html"
  },
  "options_page": "src/options.html"
}
